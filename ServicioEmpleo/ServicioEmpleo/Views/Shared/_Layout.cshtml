<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Servicio de Empleo</title>
    <!-- Bootstrap -->
    <link href="@Url.Content("~/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <!-- Bootstrap -->
    <!-- Main Style -->
    <link href="@Url.Content("~/style.css")" rel="stylesheet">
    <!-- Main Style -->
    <!-- fonts -->
    <link href='http://fonts.googleapis.com/css?family=Nunito:300,400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,700' rel='stylesheet' type='text/css'>
    <link href="@Url.Content("~/font-awesome/css/font-awesome.css")" rel="stylesheet" type="text/css">
    <!-- fonts -->
    <!-- Owl Carousel -->
    <link href="@Url.Content("~/css/owl.carousel.css")" rel="stylesheet">
    <link href="@Url.Content("~/css/owl.theme.css")" rel="stylesheet">
    <link href="@Url.Content("~/css/owl.transitions.css")" rel="stylesheet">

    <!-- Owl Carousel -->
    <!-- Form Slider -->
    <link rel="stylesheet" href="@Url.Content("~/css/jslider.css")" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/css/jslider.round.css")" type="text/css">
    <!-- Form Slider -->
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="wrapper">
        <!-- start main wrapper -->
        <div id="header">
            <!-- start main header -->
            <div class="top-line">&nbsp;</div>
            <div class="top">
                <!-- top -->
                <div class="container">
                    <div class="media-top-right">
                        <ul class="media-top clearfix">
                            <li class="item"><a href="" target="blank"><i class="fa fa-twitter"></i></a></li>
                            <li class="item"><a href="" target="blank"><i class="fa fa-facebook"></i></a></li>
                            <li class="item"><a href="" target="blank"><i class="fa fa-linkedin"></i></a></li>
                            <li class="item"><a href="" target="blank"><i class="fa fa-rss"></i></a></li>
                            <li class="item"><a href="" target="blank"><i class="fa fa-google-plus"></i></a></li>
                        </ul>
                        <ul class="media-top-2 clearfix">
                            <li><a href="" class="btn btn-default btn-blue btn-sm">Registrarse</a></li>
                            <li><a href="@Url.Action("InicioSesion", "Empleo")" class="btn btn-default btn-green btn-sm">Entrar</a></li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div><!-- top -->
            <div class="container">
                <!-- container -->
                <div class="row">
                    <div class="col-md-4">
                        <!-- logo -->
                        <a href="@Url.Content("~/Home")" title="Job Board" rel="home">
                            <img class="main-logo" src="@Url.Content("~/Images/logo.png")" alt="job board" />
                        </a>
                    </div><!-- logo -->
                    <div class="col-md-8 main-nav">
                        <!-- Main Navigation -->
                        <a id="touch-menu" class="mobile-menu" href="#"><i class="fa fa-bars fa-2x"></i></a>
                        <nav>
                            <ul class="menu">

                                <li><a href="@Url.Content("~/Home")">INICIO</a></li>
                                <li>
                                    <a href="#">SERVICIO DE EMPLEO</a>
                                    <ul class="sub-menu">
                                        <li><a href="#">ABC Servicio Público de Empleo</a></li>
                                        <li><a href="#">Certificado De Inscripción</a></li>
                                        <li><a href="#">Para Estudiantes</a></li>
                                    </ul>
                                </li>
                                <li><a href="#">OPERADORES AUTORIZADOS</a></li>
                                <li><a href="#">FORMACIÓN</a></li>
                            </ul>
                        </nav>
                    </div><!-- Main Navigation -->
                    <div class="clearfix"></div>
                </div>
            </div><!-- container -->
        </div><!-- end main header -->

        @RenderBody()

        <div id="footer">
            <!-- Footer -->
            <div class="container">
                <!-- Container -->
                <div class="row">
                    <div class="col-md-3 footer-widget">
                        <!-- Text Widget -->
                        <h6 class="widget-title">Unidad Administrativa Especial del Servicio Público de Empleo</h6>
                        <div class="textwidget">
                            <p>
                                Cra. 69 # 25B-44 Piso 3. Bogotá D.C
                                PBX:+5717560009
                                contacto@serviciodeempleo.gov.co
                            </p>
                        </div>
                    </div><!-- Text Widget -->

                    <div class="col-md-2 footer-widget">
                        <!-- Footer Menu Widget -->
                        <h6 class="widget-title">Enlaces Útiles</h6>
                        <div class="footer-widget-nav">
                            <ul>
                                <li><a href="#">Home</a></li>
                                <li><a href="#">Tips de Empleo</a></li>
                                <li><a href="#">Contáctenos</a></li>
                            </ul>
                        </div>
                    </div><!-- Footer Menu Widget -->

                    <div class="col-md-4 footer-widget">
                        <!-- Recent Tweet Widget -->
                        <h6 class="widget-title">Atención al Ciudadano</h6>
                        <div class="textwidget">
                            <p>
                                Fuera de Bogotá: 018000513100
                                En Bogotá: 4893900 Opc. 2
                                Desde su celular marque 120
                                L - V de 7 am - 7 pm, Sáb. 8 - 11 am
                                Buzón 24 horas
                            </p>
                        </div>
                    </div><!-- Recent Tweet Widget -->

                    <div class="col-md-3 footer-widget">
                        <!-- News Leter Widget -->
                        <h6 class="widget-title">Suscríbase para recibir actualizaciones</h6>
                        <div class="textwidget">
                            <p>A través de nuestro newsletter recibirá las últimas ofertas de empleo vigentes</p>
                        </div>

                        <form role="form">
                            <div class="form-group">
                                <input type="email" class="input-newstler">
                            </div>
                            <button type="button" class="btn-newstler">Suscribirse</button>
                        </form>
                        <a href="" target="blank"><i class="media-footer footer-twitt"></i></a>
                        <a href="" target="blank"><i class="media-footer footer-fb"></i></a>
                        <a href="" target="blank"><i class="media-footer footer-linkedin"></i></a>
                        <a href="" target="blank"><i class="media-footer footer-yahoo"></i></a>
                        <a href="" target="blank"><i class="media-footer footer-blog"></i></a>
                        <a href="" target="blank"><i class="media-footer footer-rss"></i></a>
                    </div><!-- News Leter Widget -->
                    <div class="clearfix"></div>
                </div>

                <div class="footer-credits">
                    <!-- Footer credits -->
                    2014 © <a href="http://redempleo.gov.co/">Servicio de Empleo</a>. Todos los derechos reservados.
                </div><!-- Footer credits -->

            </div><!-- Container -->
        </div><!-- Footer -->
    </div><!-- end main wrapper -->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="@Url.Content("~/bootstrap/js/bootstrap.min.js")"></script>

    <!-- Tabs -->
    <script src="@Url.Content("~/js/jquery.easytabs.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/modernizr.custom.49511.js")"></script>
    <!-- Tabs -->
    <!-- Owl Carousel -->
    <script src="@Url.Content("~/js/owl.carousel.js")"></script>
    <!-- Owl Carousel -->
    <!-- Form Slider -->
    <script type="text/javascript" src="@Url.Content("~/js/jshashtable-2.1_src.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/jquery.numberformatter-1.2.3.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/tmpl.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/jquery.dependClass-0.1.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/draggable-0.1.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/jquery.slider.js")"></script>
    <!-- Form Slider -->
    <!-- Map -->
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <!-- Map -->

    <script src="@Url.Content("~/js/job-board.js")"></script>

    <script type="text/javascript">
        function jobDetail() {
            window.location.href = "job-detail.html";
        }
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>

